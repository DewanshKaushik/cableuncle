                        android:text="Stop" />

                    <TextView
                        android:id="@+id/tempfilepath"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000000" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/filechooser"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Path" />

                        <Button
                            android:id="@+id/getSerialno"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="WSQScan"
                            android:textColor="#000000" />

                        <Button
                            android:id="@+id/getVersionno"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Convert WSQ"
                            android:textColor="#000000"
                            android:visibility="gone" />

                        <Button
                            android:id="@+id/matchwsq"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Match WSQ"
                            android:textColor="#000000" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Slot Number : "
                        android:textColor="#000000" />

                    <Spinner
                        android:id="@+id/slotnumberspinner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:entries="@array/slotnumber" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Smart Card Type : "
                        android:textColor="#000000" />

                    <Spinner
                        android:id="@+id/cardtypespinner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:entries="@array/cardtype" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <!--
                         <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="User Id : " />
                    -->

                    <EditText
                        android:id="@+id/useridedittext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Fill data to be written on smart card" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/smread"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Read" />

                    <Button
                        android:id="@+id/smwrite"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Write" />

                    <Button
                        android:id="@+id/smstop"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:enabled="false"
                        android:text="Stop" />
                </LinearLayout>
            </LinearLayout>

