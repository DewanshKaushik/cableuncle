                    android:textColor="#000000"></CheckBox>

                <CheckBox
                    android:id="@+id/RadioButtonDedup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="false"
                    android:text="Deduplication"
                    android:textColor="#000000"></CheckBox>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Template Format : "
                        android:textColor="#000000" />

                    <Spinner
                        android:id="@+id/fpstemplatespinner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:entries="@array/templateformat" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/fpsscan"
